<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="557.0" prefWidth="796.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.CourseController">
    <children>
        <Button fx:id="take_attendence_btn" layoutX="664.0" layoutY="29.0" mnemonicParsing="false" onAction="#onTakeAttendance" text="Take Attendence" />
        <TableView layoutX="14.0" layoutY="123.0" prefHeight="411.0" prefWidth="767.0">
            <columns>
                <TableColumn prefWidth="160.0" text="Date" />
                <TableColumn minWidth="0.0" prefWidth="211.0" text="Total Present" />
            <TableColumn prefWidth="231.0" text="Total Absent" />
            </columns>
        </TableView>
        <Button fx:id="go_back" layoutX="14.0" layoutY="29.0" mnemonicParsing="false" onAction="#onGoback" text="&lt; Go Back" />
      <Button fx:id="assign_student" layoutX="526.0" layoutY="29.0" mnemonicParsing="false" onAction="#onAssignStudent" text="+ Assign Student" />
    </children>
</AnchorPane>
