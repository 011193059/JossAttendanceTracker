<AnchorPane prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.AdminCourseController">
    <children>
        <TableView fx:id="course_table" layoutX="39.0" layoutY="101.0" prefHeight="432.0" prefWidth="822.0">
            <columns>
                <TableColumn fx:id="name" prefWidth="145.0" text="Name" />
                <TableColumn fx:id="section" minWidth="0.0" prefWidth="152.0" text="Section" />
                <TableColumn fx:id="department" prefWidth="195.0" text="Department" />
                <TableColumn fx:id="subject" minWidth="0.0" prefWidth="142.0" text="Subject" />
            </columns>
        </TableView>
        <Button fx:id="go_back" layoutX="39.0" layoutY="29.0" mnemonicParsing="false" onAction="#onGoback" style="-fx-background-color: purple;" text="&lt; Go Back" textFill="WHITE">
         <font>
            <Font name="Calibri Bold" size="13.0" />
         </font></Button>
      <Button fx:id="create_new_class" layoutX="752.0" layoutY="29.0" mnemonicParsing="false" onAction="#onCreateNewClass" style="-fx-background-color: purple;" text="Create new class" textFill="WHITE">
         <font>
            <Font name="Calibri Bold" size="13.0" />
         </font></Button>
    </children>
</AnchorPane>
